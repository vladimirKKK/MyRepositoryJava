package Test;
import ru.relex.education.time.*;
import ru.relex.education.time.Time;
public class FormatTime {

    public static void main(String[] args) {
        long zero = 1363665600;
        long example = 1363687063;
        //Time time =  Utils.splite(1363687063-1363665600);
        Time t = Utils.splite(1363687063-1363665600);
        System.out.println(t.getMinutes() + " : " + t.getSeconds());
    }

}
